APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
# URL dasar aplikasi Laravel.
# Default-nya saat menjalankan `php artisan serve` adalah: http://127.0.0.1:8000
# Kalau kamu pakai cara biasa, cukup biarkan ini seperti ini.
# 
# Namun, jika kamu menjalankan server Laravel dengan IP lokal (misal untuk tes dari HP atau jaringan LAN),
# seperti menjalankan: `php artisan serve --host=0.0.0.0`
# Maka kamu harus mengganti APP_URL ini menjadi IP lokal dari komputermu.
#
# Caranya:
# 1. Buka terminal/CMD dan ketik: `ipconfig` (Windows) atau `ifconfig` (Linux/Mac)
# 2. Cari bagian `IPv4 Address`, contoh: `192.168.1.10`
# 3. Ganti APP_URL menjadi: http://192.168.1.10:8000
#
# Kenapa ini penting? Karena:
# - APP_URL digunakan saat menampilkan gambar atau file yang disimpan di database hanya berupa path.
# - Jika APP_URL salah, gambar/file tidak bisa diakses dengan benar dari browser.
APP_URL=http://127.0.0.1:8000


APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
